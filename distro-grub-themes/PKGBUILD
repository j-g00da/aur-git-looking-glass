prepare() {
  for t in *.tar; do
    bn="$(basename "$t" .tar)"
    mkdir -p "$bn"
    tar -xf "$t" -C "$bn"
  done
}

package_grub-theme-acer() {
  cd "${pkgname}-${pkgver}"
  name=$(echo "${pkgname}" | sed -e "s/\\b\(.\)/\u\1/g" | sed -e "s/^Grub-Theme-//")
  install -d "$pkgdir"/usr/share/grub/themes/$name
  pkgdesc="${name} Theme for Grub2"
  cp -a * "$pkgdir"/usr/share/grub/themes/$name
}

