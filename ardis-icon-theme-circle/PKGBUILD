pkgname=ardis-icon-theme-circle
pkgver=1.2
pkgrel=0
pkgdesc="Ardis circle icons theme"
url="http://gnome-look.org/content/show.php/Ardis+circle+%26+square?content=169815"
license=('GPL2')
arch=('any')
source=("http://esclapion.free.fr/AUR/$pkgname.tar.gz")
md5sums=('7c79bb7cbf95c23be48322961e44886f')

package(){
  mkdir -p $pkgdir/usr/share/icons
  cp -a $srcdir/$pkgname $pkgdir/usr/share/icons
}
