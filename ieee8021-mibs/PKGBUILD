package() {
  for mib in "${srcdir}"/*.mib; do
    _output="$(smidump -k "${mib}" 2> /dev/null \
        | grep DEFINITIONS\
        | awk '{print $1;}'
    )"
    install -Dm644 "${mib}" "${pkgdir}/usr/share/mibs/ieee/${_output}"
  done
  install -Dm644 "${srcdir}/smi.conf" "${pkgdir}/etc/smi.conf"
}
