package() {
  # We have to use the v1 packages, the v2 packages don't currently work with vs-mlrt
  cd "rife"

  for i in $(ls *.onnx); do install -Dm644 "${i}" "${pkgdir}/usr/lib/vapoursynth/models/rife/${i}"; done
}
