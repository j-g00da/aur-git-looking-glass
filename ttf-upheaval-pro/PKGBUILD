# ttf-upheaval-pro
# Maintainer: Sterophonick

pkgname=ttf-upheaval-pro
pkgver=1
pkgrel=1
arch=('any')
pkgdesc="Extended version of Upheaval by Brian Kent"
source=(UpheavalPro.ttf)
md5sums=('fe7a6c07347edd2abc2d917fde89dd9b')

package() {
	mkdir "${pkgdir}/usr"
    mkdir "${pkgdir}/usr/share"
    mkdir "${pkgdir}/usr/share/fonts"
    cp UpheavalPro.ttf "${pkgdir}/usr/share/fonts"
}

