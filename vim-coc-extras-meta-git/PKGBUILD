pkgname=vim-coc-extras-meta-git
pkgver=1.0
pkgrel=1
pkgdesc='Meta package to install some basic extensions for coc.nvim'
arch=(any)
url='https://github.com/neoclide/coc.nvim'
provides=('vim-coc-extras-meta')
conflicts=('vim-coc-extras-meta')
depends=(vim-coc-highlight-git
         vim-coc-lists-git
         vim-coc-snippets-git
         vim-coc-syntax-git
         vim-coc-dictionary-git
         vim-coc-tag-git
         vim-coc-yank-git)
optdepends=('vim-coc-word-git: words from google 10000 english repo'
            'vim-coc-emoji-git: emoji words for markdown'
            'vim-coc-pairs-git: auto pairs support'
            'vim-coc-explorer-git: file/buffer explorer'
            'vim-coc-git-git: git integration')
